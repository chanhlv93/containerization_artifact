{
   "AWSEBDockerrunVersion": "2",
   "containerDefinitions": [
      {
         "name": "vote",
         "image": "chanhlv93/docker-voting",
         "essential": true,
         "memory": 128,
         "portMappings": [
            {
               "hostPort": 5000,
               "containerPort": 80
            }
         ],
         "links": [
            "redis"
         ]
      },
      {
         "name": "result",
         "image": "chanhlv93/docker-result",
         "essential": true,
         "memory": 128,
         "portMappings": [
            {
               "hostPort": 5001,
               "containerPort": 80
            },
            {
               "hostPort": 5858,
               "containerPort": 5858
            }
         ],
         "links": [
            "db"
         ]
      },
      {
         "name": "worker",
         "image": "chanhlv93/docker-worker",
         "essential": true,
         "memory": 128,
         "links": [
            "redis",
            "db"
         ]
      },
      {
         "name": "redis",
         "image": "redis:alpine",
         "essential": true,
         "memory": 128,
         "portMappings": [
            {
               "hostPort": 6379,
               "containerPort": 6379
            }
         ]
      },
      {
         "name": "db",
         "image": "postgres:9.4",
         "essential": true,
         "memory": 128,
         "portMappings": [
            {
               "hostPort": 5524,
               "containerPort": 5524
            }
         ]
      }
   ]
}